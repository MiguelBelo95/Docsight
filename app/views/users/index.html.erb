<div class="container text-center">
</div>
  <h1>Select your prefered doctor!</h1>
  <% @doctors.each do |user| %>
    <% full_name = "#{user.first_name} #{user.last_name}" %>
    <div class="card-product my-3 btn">
      <%= image_tag user.image_url %>
      <div class="card-product-infos">
        <%= user.first_name%>
        <%= user.last_name%>
        <%= user.locations.first.name %>
        <%= user.locations.first.address %>
      </div>
    </div>
<div>
<% end %>
